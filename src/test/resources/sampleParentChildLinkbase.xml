      <clink:concept qname="kvk-abstr:StatementOfComprehensiveIncomeTitle" cxlink:type="resource"/>
      <clink:concept qname="rj-i:ComprehensiveIncomeOther" cxlink:type="resource"/>
    </clink:presentationArc>
    <clink:presentationArc order="3" cxlink:arcrole="http://www.xbrl.org/2003/arcrole/parent-child" cxlink:type="arc">
      <clink:concept qname="kvk-abstr:StatementOfComprehensiveIncomeTitle" cxlink:type="resource"/>
      <clink:concept qname="venj-bw2-i:ComprehensiveIncome" cxlink:type="resource"/>
    </clink:presentationArc>
    <clink:presentationArc order="4" cxlink:arcrole="http://www.xbrl.org/2003/arcrole/parent-child" cxlink:type="arc">
