<link:linkbase
  xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:link="http://www.xbrl.org/2003/linkbase"
  xmlns:generic="http://xbrl.org/2008/generic"
  xmlns:ea="http://xbrl.org/2008/assertion/existence"
  xmlns:variable="http://xbrl.org/2008/variable"
  xmlns:msg="http://xbrl.org/2010/message"
  xmlns:xfi="http://www.xbrl.org/2008/function/instance" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:xs="http://www.w3.org/2001/XMLSchema" 
  xmlns:test="http://abc.com/test"
  xmlns:xbrli="http://www.xbrl.org/2003/instance"
  xmlns:v="http://xbrl.org/2008/formula/conformance/variable"

  xsi:schemaLocation="
  http://www.xbrl.org/2003/linkbase http://www.xbrl.org/2003/xbrl-linkbase-2003-12-31.xsd
  http://xbrl.org/2008/assertion/existence ../../../core_schemas/2008/existence-assertion.xsd
  http://xbrl.org/2008/generic ../../../core_schemas/2008/generic-link.xsd
  http://xbrl.org/2008/formula ../../../core_schemas/2008/formula.xsd
  http://xbrl.org/2008/variable ../../../core_schemas/2008/variable.xsd
  http://xbrl.org/2010/message ../../../core_schemas/2010/generic-message.xsd
  ">
  
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-set" xlink:href="../../../core_schemas/2008/variable.xsd#variable-set" xlink:type="simple"/>
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-filter" xlink:href="../../../core_schemas/2008/variable.xsd#variable-filter" xlink:type="simple"/>
  <link:roleRef roleURI="http://www.xbrl.org/2008/role/link" xlink:href="../../../core_schemas/2008/generic-link.xsd#standard-link-role" xlink:type="simple"/>
  <link:roleRef roleURI="http://www.xbrl.org/2010/role/message" xlink:href="../../../core_schemas/2010/generic-message.xsd#standard-message" xlink:type="simple"/>
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2010/assertion-unsatisfied-message" xlink:href="../../../core_schemas/2010/validation-message.xsd#assertion-unsatisfied-message" xlink:type="simple"/>

  <generic:link xlink:type="extended"
    xlink:role="http://www.xbrl.org/2008/role/link">

    <ea:existenceAssertion xlink:type="resource" xlink:label="test-assertion" id="test-assertion" aspectModel="non-dimensional"
     implicitFiltering="false" test=". ge $v:existenceMinOccurs and . le $maxOccurs" />

    <msg:message xlink:type="resource" xlink:label="test-assertion-unsatisfied-message" xlink:role="http://www.xbrl.org/2010/role/message" 
    xml:lang="en">Not satisfied error: The expression '{$ea:test-expression}' was not satisfied. Number of evaluations was: {.}. Max occurs param was: {$maxOccurs}.</msg:message>   
                                                                        
    <generic:arc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2010/assertion-unsatisfied-message"
      xlink:from="test-assertion" xlink:to="test-assertion-unsatisfied-message" order="1.0"/>

    <!-- Param referenced globally by its @name -->
    <variable:parameter xlink:type="resource" xlink:label="globalParamMaxOccurs" name="maxOccurs" select="100" />
    
    <!-- Param referenced by the name supplied on its variable-set arc -->
    <variable:parameter xlink:type="resource" xlink:label="paramMinOccurs" name="minOccurs" select="2" />
    
    <variable:factVariable xlink:type="resource" xlink:label="var"   bindAsSequence="false" />

    <variable:variableArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" 
      xlink:from="test-assertion" xlink:to="var" order="1.0"  name="var" />
      
    <variable:variableArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" 
      xlink:from="test-assertion" xlink:to="paramMinOccurs" order="1.0"  name="v:existenceMinOccurs" />

  </generic:link>

</link:linkbase>
