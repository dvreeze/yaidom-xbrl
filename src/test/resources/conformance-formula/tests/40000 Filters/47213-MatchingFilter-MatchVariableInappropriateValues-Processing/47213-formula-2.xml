<?xml version="1.0" encoding="utf-8"?>
<link:linkbase 
  xmlns:link="http://www.xbrl.org/2003/linkbase" 
  xmlns:generic="http://xbrl.org/2008/generic" 
  xmlns:formula="http://xbrl.org/2008/formula" 
  xmlns:variable="http://xbrl.org/2008/variable" 
  xmlns:mf="http://xbrl.org/2008/filter/match" 
  xmlns:cf="http://xbrl.org/2008/filter/concept" 
  xmlns:label="http://xbrl.org/2008/label" 
  xmlns:v="http://xbrl.org/2008/formula/conformance/variable"
  xmlns:eg="http://xbrl.org/formula/conformance/example" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xmlns:xlink="http://www.w3.org/1999/xlink" 
  xmlns:tst="http://example.com/test"
  xsi:schemaLocation="
  http://www.xbrl.org/2003/linkbase
    http://www.xbrl.org/2003/xbrl-linkbase-2003-12-31.xsd
  http://xbrl.org/2008/generic
    ../../../core_schemas/2008/generic-link.xsd 
  http://xbrl.org/2008/formula
    ../../../core_schemas/2008/formula.xsd
  http://xbrl.org/2008/variable
    ../../../core_schemas/2008/variable.xsd
  http://xbrl.org/2008/filter/match
    ../../../core_schemas/2008/match-filter.xsd
  http://xbrl.org/2008/label
    ../../../core_schemas/2008/generic-label.xsd">
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-set" xlink:href="../../../core_schemas/2008/variable.xsd#variable-set" xlink:type="simple"/>
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-filter" xlink:href="../../../core_schemas/2008/variable.xsd#variable-filter" xlink:type="simple"/>  <link:roleRef roleURI="http://www.xbrl.org/2008/role/link" xlink:href="../../../core_schemas/2008/generic-link.xsd#standard-link-role" xlink:type="simple"/>

  <generic:link xlink:type="extended" xlink:role="http://www.xbrl.org/2008/role/link">
    <formula:formula xlink:type="resource" xlink:label="formula1" 
      value="$v:a" source="v:a"
      aspectModel="non-dimensional" implicitFiltering="false">
    </formula:formula>
      
    <variable:factVariable xlink:type="resource" xlink:label="factvarA" bindAsSequence="false"  />
    <variable:factVariable xlink:type="resource" xlink:label="factvarB" bindAsSequence="true" matches="true" />

    <!-- match filter for fact var A -->
    <mf:matchPeriod xlink:type="resource" xlink:label="matchPeriod" variable="v:b" />

    <!-- concept name filters -->
    <cf:conceptName xlink:type="resource" xlink:label="filterA">
      <cf:concept>
        <cf:qname>tst:FactA</cf:qname>
      </cf:concept>
    </cf:conceptName>
    
    <cf:conceptName xlink:type="resource" xlink:label="filterB">
      <cf:concept>
        <cf:qname>tst:FactB</cf:qname>
      </cf:concept>
    </cf:conceptName>

    <variable:variableArc xlink:type="arc" xlink:to="factvarA" xlink:from="formula1" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" order="1" 
      name="v:a" />
    <variable:variableArc xlink:type="arc" xlink:to="factvarB" xlink:from="formula1" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" order="2" 
      name="v:b" />
    <variable:variableFilterArc xlink:type="arc" xlink:to="matchPeriod" xlink:from="factvarA" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" order="2" complement="false" cover="false" />
    <variable:variableFilterArc xlink:type="arc" xlink:to="filterA" xlink:from="factvarA" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" order="1" complement="false" cover="false" />
    <variable:variableFilterArc xlink:type="arc" xlink:to="filterB" xlink:from="factvarB" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" order="1" complement="false" cover="false" />
 
  </generic:link>
</link:linkbase>




