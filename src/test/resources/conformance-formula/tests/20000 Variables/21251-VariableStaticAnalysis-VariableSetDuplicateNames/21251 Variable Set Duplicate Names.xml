<?xml-stylesheet type="text/xsl" href="../../../infrastructure/test.xsl"?>
<testcase
  xmlns="http://xbrl.org/2008/conformance"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xbrl.org/2008/conformance ../../../infrastructure/test.xsd"
  xmlns:xbrlfe="http://xbrl.org/2008/formula/error"
  xmlns:binding="http://xbrl.org/2008/binding/errors"
  xmlns:label="http://xbrl.org/2008/label/error"
  xmlns:reference="http://xbrl.org/2008/reference/error"  
  >
  <creator>
    <name>Stuart Barker</name>
    <email>dsb@corefiling.com</email>
  </creator>
  <number>21251</number>
  <name>Variable Set Duplicate Names</name>
  <description>
    Test for duplicate names in variable sets.
  </description>
  <reference specification="VARIABLES" id="sec-variable-sets"/>

  <variation id="V-01">
    <name>Duplicate fact variable name in variable set</name>
    <description>Tests that duplicate fact variable names in a variable set generate an error.</description>
    <reference specification="VARIABLES" id="sec-variable-sets"/>
    <data>
      <schema>21251-factVar-tests.xsd</schema>
      <linkbase>21251-duplicateFactVariableNameInSet-formula.xml</linkbase>
      <instance readMeFirst="true">21251-duplicateFactVariableNameInSet-instance.xml</instance>
    </data>
    <result>
      <error xmlns:xbrlve="http://xbrl.org/2008/variable/error">xbrlve:duplicateVariableNames</error>
    </result>
  </variation>  

  <variation id="V-02">
    <name>Duplicate general variable name in variable set</name>
    <description>Tests that duplicate general variable names in a variable set generate an error.</description>
    <reference specification="VARIABLES" id="sec-variable-sets"/>
    <data>
      <schema>21251-factVar-tests.xsd</schema>
      <linkbase>21251-duplicateGeneralVariableNameInSet-formula.xml</linkbase>
      <instance readMeFirst="true">21251-duplicateGeneralVariableNameInSet-instance.xml</instance>
    </data>
    <result>
      <error xmlns:xbrlve="http://xbrl.org/2008/variable/error">xbrlve:duplicateVariableNames</error>
    </result>
  </variation>   

  <variation id="V-03">
    <name>Duplicate parameter name in variable set</name>
    <description>Tests that duplicate parameter names in a variable set generate an error.</description>
    <reference specification="VARIABLES" id="sec-variable-sets"/>
    <data>
      <schema>21251-factVar-tests.xsd</schema>
      <linkbase>21251-duplicateParameterNameInSet-formula.xml</linkbase>
      <instance readMeFirst="true">21251-duplicateParameterNameInSet-instance.xml</instance>
    </data>
    <result>
      <error xmlns:xbrlve="http://xbrl.org/2008/variable/error">xbrlve:duplicateVariableNames</error>
    </result>
  </variation>    

  <variation id="V-04">
    <name>Variable with same name as global parameter</name>
    <description>Tests that a variable with the same name as global parameter doesn't generate an error.</description>
    <reference specification="VARIABLES" id="sec-variable-sets"/>
    <data>
      <schema>21251-factVar-tests.xsd</schema>
      <linkbase>21251-VariableNameIsSameAsGlobalParameter-formula.xml</linkbase>
      <instance readMeFirst="true">21251-VariableNameIsSameAsGlobalParameter-instance.xml</instance>
    </data>
    <result>
      <assertionTests 
         assertionID="assertion"
         countSatisfied="1"
         countNotSatisfied="0" />
    </result>
  </variation>   

  <variation id="V-05">
    <name>Local parameter with same name as global parameter</name>
    <description>Tests that a local parameter with the same name as global parameter doesn't generate an error.</description>
    <reference specification="VARIABLES" id="sec-variable-sets"/>
    <data>
      <schema>21251-factVar-tests.xsd</schema>
      <linkbase>21251-ParameterNameIsSameAsGlobalParameter-formula.xml</linkbase>
      <instance readMeFirst="true">21251-ParameterNameIsSameAsGlobalParameter-instance.xml</instance>
    </data>
    <result>
      <assertionTests 
         assertionID="assertion"
         countSatisfied="1"
         countNotSatisfied="0" />
      <assertionTests 
         assertionID="assertion2"
         countSatisfied="1"
         countNotSatisfied="0" />
    </result>
  </variation>    

  <variation id="V-06">
    <name>Same variable name in different variable sets</name>
    <description>Tests that the same variable name used in two different variable sets doesn't generate an error.</description>
    <reference specification="VARIABLES" id="sec-variable-sets"/>
    <data>
      <schema>21251-factVar-tests.xsd</schema>
      <linkbase>21251-SameVariableNamesInSeperateSets-formula.xml</linkbase>
      <instance readMeFirst="true">21251-SameVariableNamesInSeperateSets-instance.xml</instance>
    </data>
    <result>
      <assertionTests 
         assertionID="assertion"
         countSatisfied="1"
         countNotSatisfied="0" />
      <assertionTests 
         assertionID="assertion2"
         countSatisfied="1"
         countNotSatisfied="0" />
    </result>
  </variation>  

 </testcase>





