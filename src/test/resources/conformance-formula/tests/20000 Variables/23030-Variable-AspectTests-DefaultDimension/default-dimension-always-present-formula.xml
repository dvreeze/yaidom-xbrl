<link:linkbase
  xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:link="http://www.xbrl.org/2003/linkbase"
  xmlns:generic="http://xbrl.org/2008/generic"
  xmlns:formula="http://xbrl.org/2008/formula"
  xmlns:validation="http://xbrl.org/2008/validation"
  xmlns:va="http://xbrl.org/2008/assertion/value"
  xmlns:variable="http://xbrl.org/2008/variable"
  xmlns:df="http://xbrl.org/2008/filter/dimension" 
  xmlns:cf="http://xbrl.org/2008/filter/concept"
  xmlns:label="http://xbrl.org/2008/label"
  xmlns:xfi="http://www.xbrl.org/2008/function/instance" 
  xmlns:v="http://xbrl.org/2008/formula/conformance/variable"
  xmlns:prim="http://xbrl.org/formula/conformance/example/prim" 
  xmlns:dim1="http://xbrl.org/formula/conformance/example/dim1"
  xmlns:dim2="http://xbrl.org/formula/conformance/example/dim2"    
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
  http://www.xbrl.org/2003/linkbase http://www.xbrl.org/2003/xbrl-linkbase-2003-12-31.xsd
  http://xbrl.org/2008/generic ../../../core_schemas/2008/generic-link.xsd
  http://xbrl.org/2008/formula ../../../core_schemas/2008/formula.xsd
  http://xbrl.org/2008/assertion/value ../../../core_schemas/2008/value-assertion.xsd
  http://xbrl.org/2008/variable ../../../core_schemas/2008/variable.xsd
  http://xbrl.org/2008/filter/concept ../../../core_schemas/2008/concept-filter.xsd
  http://xbrl.org/2008/filter/period ../../../core_schemas/2008/period-filter.xsd
  http://xbrl.org/2008/label ../../../core_schemas/2008/generic-label.xsd
  ">
  
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-set" xlink:href="../../../core_schemas/2008/variable.xsd#variable-set" xlink:type="simple"/>
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-filter" xlink:href="../../../core_schemas/2008/variable.xsd#variable-filter" xlink:type="simple"/>
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-set-precondition" xlink:href="../../../core_schemas/2008/variable.xsd#variable-set-precondition" xlink:type="simple"/>
  <link:roleRef roleURI="http://www.xbrl.org/2008/role/link" xlink:href="../../../core_schemas/2008/generic-link.xsd#standard-link-role" xlink:type="simple"/>

  <generic:link xlink:type="extended"
    xlink:role="http://www.xbrl.org/2008/role/link">

    <va:valueAssertion xlink:type="resource" xlink:label="assertion" 
        id="assertion"
        aspectModel="dimensional" implicitFiltering="true"
        test="abs($v:n1 - $v:n2 - $v:n3 - $v:n4) le 0"/>

    <!--  Fact variables.  Note that no general variables are used in this formula. -->
    <variable:factVariable xlink:type="resource" xlink:label="factVarn1"  bindAsSequence="false"/>
    <variable:factVariable xlink:type="resource" xlink:label="factVarn2"  bindAsSequence="false"/>
    <variable:factVariable xlink:type="resource" xlink:label="factVarn3"  bindAsSequence="false"/>
	<variable:factVariable xlink:type="resource" xlink:label="factVarn4"  bindAsSequence="false"/>

    <!--  Concept filters for the fact variables -->
    <cf:conceptName xlink:type="resource" xlink:label="nameFiltern1">
      <cf:concept>
        <cf:qname>prim:n1</cf:qname>
      </cf:concept>
    </cf:conceptName> 
    <cf:conceptName xlink:type="resource" xlink:label="nameFiltern2">
      <cf:concept>
        <cf:qname>prim:n2</cf:qname>
      </cf:concept>
    </cf:conceptName> 
    <cf:conceptName xlink:type="resource" xlink:label="nameFiltern3">
      <cf:concept>
        <cf:qname>prim:n3</cf:qname>
      </cf:concept>
    </cf:conceptName> 
	
	<cf:conceptName xlink:type="resource" xlink:label="nameFiltern4">
      <cf:concept>
        <cf:qname>prim:n4</cf:qname>
      </cf:concept>
    </cf:conceptName> 
	
	
	<df:explicitDimension xlink:type="resource" xlink:label="Dim_Filter_1">
      <df:dimension>
        <df:qname>dim1:RegionDimension</df:qname>
      </df:dimension>
      <df:member>
        <df:qname>dim1:TotalRegions</df:qname>
      </df:member>
    </df:explicitDimension>
	
	<df:explicitDimension xlink:type="resource" xlink:label="Dim_Filter_2">
      <df:dimension>
        <df:qname>dim2:SeasonDimension</df:qname>
      </df:dimension>
      <df:member>
        <df:qname>dim2:TotalSeasons</df:qname>
      </df:member>
    </df:explicitDimension>
	
	

    <!--  Arc from the formula to the variables it depends on -->
    <variable:variableArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" 
      name="v:n1" xlink:from="assertion" xlink:to="factVarn1" order="1.0"/>
    <variable:variableArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" 
      name="v:n2" xlink:from="assertion" xlink:to="factVarn2" order="1.0"/>
    <variable:variableArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" 
      name="v:n3" xlink:from="assertion" xlink:to="factVarn3" order="1.0"/>
	<variable:variableArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" 
      name="v:n4" xlink:from="assertion" xlink:to="factVarn4" order="1.0"/>

    <!--  Arcs from variables to their concept filters -->
    <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" 
      complement="false" cover="true"
      xlink:from="factVarn1" xlink:to="nameFiltern1" order="1.0"/>
    <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" 
      complement="false" cover="true"
      xlink:from="factVarn2" xlink:to="nameFiltern2" order="1.0"/>
    <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" 
      complement="false" cover="true"
      xlink:from="factVarn3" xlink:to="nameFiltern3" order="1.0"/>
	<variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" 
      complement="false" cover="true"
      xlink:from="factVarn4" xlink:to="nameFiltern4" order="1.0"/>
	  
	  
	<!--  Arcs from variables to their dimension filters -->  
	
	<variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" 
      complement="false" cover="true"
      xlink:from="factVarn1" xlink:to="Dim_Filter_1" order="1.0"/>
    <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" 
      complement="false" cover="true"
      xlink:from="factVarn2" xlink:to="Dim_Filter_1" order="1.0"/>
    <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" 
      complement="false" cover="true"
      xlink:from="factVarn3" xlink:to="Dim_Filter_1" order="1.0"/>
	<variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" 
      complement="false" cover="true"
      xlink:from="factVarn4" xlink:to="Dim_Filter_1" order="1.0"/>
	  
	  
	 <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" 
      complement="false" cover="true"
      xlink:from="factVarn1" xlink:to="Dim_Filter_2" order="1.0"/>
    <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" 
      complement="false" cover="true"
      xlink:from="factVarn2" xlink:to="Dim_Filter_2" order="1.0"/>
    <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" 
      complement="false" cover="true"
      xlink:from="factVarn3" xlink:to="Dim_Filter_2" order="1.0"/>
	<variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" 
      complement="false" cover="true"
      xlink:from="factVarn4" xlink:to="Dim_Filter_2" order="1.0"/>


  </generic:link>

</link:linkbase>

