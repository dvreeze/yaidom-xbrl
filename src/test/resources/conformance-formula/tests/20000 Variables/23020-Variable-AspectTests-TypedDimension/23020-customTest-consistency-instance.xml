<xbrl 
xmlns="http://www.xbrl.org/2003/instance" 
xmlns:t="http://xbrl.org/formula/conformance/example"
xmlns:link="http://www.xbrl.org/2003/linkbase" 
xmlns:xlink="http://www.w3.org/1999/xlink" 
xmlns:xbrldi="http://xbrl.org/2006/xbrldi"
xmlns:iso4217="http://www.xbrl.org/2003/iso4217" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="
  http://xbrl.org/2006/xbrldi http://www.xbrl.org/2006/xbrldi-2006.xsd
"
 >
  
  <link:schemaRef 
    xlink:type="simple" 
    xlink:arcrole="http://www.w3.org/1999/xlink/properties/linkbase" 
    xlink:href="23020-typedDim-aspects.xsd"/>

  <link:linkbaseRef 
    xlink:type="simple" 
    xlink:arcrole="http://www.w3.org/1999/xlink/properties/linkbase" 
    xlink:href="23020-customTest-consistency-formula.xml"/>

  <link:linkbaseRef 
    xlink:type="simple" 
    xlink:arcrole="http://www.w3.org/1999/xlink/properties/linkbase" 
    xlink:href="23020-customTest-ignoreOrder-aspectTest.xml"/>

  <context id="c1">
    <entity>
      <identifier scheme="http://xbrl.org/entity/identification/scheme">01</identifier>
      <segment>
        <xbrldi:typedMember dimension="t:typeDim1">
          <t:dimVal >
            <t:strVal>abc</t:strVal>
            <t:intVal>123</t:intVal>
            <t:qnameVal>t:test</t:qnameVal>
          </t:dimVal>
        </xbrldi:typedMember>
      </segment>
    </entity>
    <period>
      <instant>2007-12-31</instant>
    </period>
  </context>  
  <context id="c2">
    <entity>
      <identifier scheme="http://xbrl.org/entity/identification/scheme">01</identifier>
      <segment>
        <xbrldi:typedMember dimension="t:typeDim1">
          <t:dimVal >
            <!-- order changed here -->
            <t:qnameVal>t:test</t:qnameVal>
            <t:intVal>123</t:intVal>
            <t:strVal>abc</t:strVal>
          </t:dimVal>
        </xbrldi:typedMember>
      </segment>
    </entity>
    <period>
      <instant>2007-12-31</instant>
    </period>
  </context>
  
  <unit id="gbp">
  	<measure>iso4217:GBP</measure>
  </unit>

  <t:P4 contextRef="c1" unitRef="gbp" decimals="2">41</t:P4>
  <t:P5 contextRef="c2" unitRef="gbp" decimals="2">42</t:P5>

</xbrl>




