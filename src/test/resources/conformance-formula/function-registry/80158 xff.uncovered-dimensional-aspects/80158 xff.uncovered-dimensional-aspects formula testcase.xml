  <number>80158</number>

  <name>xff:uncovered-dimensional-aspects</name>

  <description>
    Tests xff:uncovered-dimensional-aspects function which can only be executed in a variable set.
  </description>

  <variation id="V-01">
    <name>All dimension aspects uncovered</name>
    <description>Test with all dimension aspect uncovered.</description>
    <reference specification="VARIABLES" id="sec-binding-to-empty-sequence"/>
    <data>
      <schema>uncovered-dim-tests.xsd</schema>
      <linkbase>all-dim-uncovered-formula.xml</linkbase>
      <instance readMeFirst="true">all-dim-uncovered-instance.xml</instance>
    </data>
    <result>
      <assertionTests 
         assertionID="assertion"
         countSatisfied="1"
         countNotSatisfied="0" />
    </result>
  </variation>  

  <variation id="V-02">
    <name>Dim1 dimension aspects uncovered</name>
    <description>Test with one dimension aspect uncovered.</description>
    <reference specification="VARIABLES" id="sec-binding-to-empty-sequence"/>
