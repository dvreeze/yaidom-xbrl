  <number>80152</number>

  <name>xfi:is-fraction</name>

  <documentation>
    <xhtml:p>
      Test the functionality of the xfi:is-fraction function.
    </xhtml:p>
  </documentation>

  <reference xlink:type="simple" 
  xlink:href="http://www.xbrl.org/Specification/XBRL-RECOMMENDATION-2003-12-31+Corrected-Errata-2006-12-18.htm#_Toc156209183"/>

  <variation id="V-01">
    <name>Returns false for a non-numeric concept</name>
    <documentation>
      <xhtml:p>
        The concept has an xbrli:stringItemType data type.
      </xhtml:p>
     </documentation>
    <inputs>
      <schema xlink:type="simple" xlink:href="types-test.xsd"/>
      <instance readMeFirst="true" id="V01" xlink:type="simple" xlink:href="types-instance.xml"/>
      <cfcn:call file="V01">
        xfi:is-fraction(fn:node-name(/xbrli:xbrl/concept:c1[1]))
      </cfcn:call>
    </inputs>
    <outputs>
      <cfcn:test>
         $result eq false()
      </cfcn:test>
    </outputs>
  </variation>

  <variation id="V-02">
    <name>Returns false for a non-fraction numeric concept</name>
    <documentation>
      <xhtml:p>
        The concept has an xbrli:monetaryItemType data type.
      </xhtml:p>
     </documentation>
    <inputs>
      <schema xlink:type="simple" xlink:href="types-test.xsd"/>
      <instance readMeFirst="true" id="V02" xlink:type="simple" xlink:href="types-instance.xml"/>
      <cfcn:call file="V02">
        xfi:is-fraction(fn:node-name(/xbrli:xbrl/concept:c2[1]))
      </cfcn:call>
    </inputs>
    <outputs>
      <cfcn:test>
         $result eq false()
      </cfcn:test>
    </outputs>
  </variation>

  <variation id="V-03">
    <name>Returns true for a fraction concept</name>
    <documentation>
      <xhtml:p>
        The concept has an xs:fractionItemType data type.
      </xhtml:p>
     </documentation>
    <inputs>
      <schema xlink:type="simple" xlink:href="types-test.xsd"/>
      <instance readMeFirst="true" id="V03" xlink:type="simple" xlink:href="types-instance.xml"/>
      <cfcn:call file="V03">
        xfi:is-fraction(fn:node-name(/xbrli:xbrl/concept:c3[1]))
      </cfcn:call>
    </inputs>
    <outputs>
      <cfcn:test>
         $result eq true()
      </cfcn:test>
    </outputs>
  </variation>

  <variation id="V-04">
    <name>Returns false for a tuple concept</name>
    <documentation>
      <xhtml:p>
        The concept is a tuple.
      </xhtml:p>
     </documentation>
    <inputs>
      <schema xlink:type="simple" xlink:href="types-test.xsd"/>
      <instance readMeFirst="true" id="V04" xlink:type="simple" xlink:href="types-instance.xml"/>
      <cfcn:call file="V04">
        xfi:is-fraction(fn:node-name(/xbrli:xbrl/concept:c4[1]))
      </cfcn:call>
    </inputs>
    <outputs>
      <cfcn:test>
         $result eq false()
      </cfcn:test>
    </outputs>
  </variation>

