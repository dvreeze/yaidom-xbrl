<?xml version="1.0" encoding="utf-8"?>
<linkbase
  xmlns="http://www.xbrl.org/2003/linkbase"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:test="http://abc.com/test"
  xsi:schemaLocation="
    http://www.xbrl.org/2003/linkbase 
      http://www.xbrl.org/2003/xbrl-linkbase-2003-12-31.xsd
    http://abc.com/test
      test.xsd
    ">
	<roleRef xlink:type="simple" 
      xlink:href="test.xsd#test-link-role" 
      roleURI="http://abc.com/role/test-link-role"/>
	<arcroleRef xlink:type="simple" 
      xlink:href="test.xsd#test-arc-role" 
      arcroleURI="http://abc.com/arcrole/test-arc-role"/>
      
	<test:testLink xlink:type="extended" 
      xlink:role="http://abc.com/role/test-link-role"
      test:testLinkDate="2012-12-21">
		<loc xlink:type="locator" 
          xlink:href="test.xsd#A" 
          xlink:label="a"/>
		<loc xlink:type="locator" 
          xlink:href="test.xsd#B" 
          xlink:label="b"/>
		<loc xlink:type="locator" 
          xlink:href="test.xsd#C" 
          xlink:label="c"/>
		<test:testArc xlink:type="arc" 
          xlink:arcrole="http://abc.com/arcrole/test-arc-role"
          xlink:from="a" xlink:to="b" order="1" 
          test:testArcInt="-3" />
		<test:testArc xlink:type="arc" 
          xlink:arcrole="http://abc.com/arcrole/test-arc-role"
          xlink:from="b" xlink:to="c" order="1" 
          test:testArcInt="4" />
	</test:testLink>
</linkbase>



