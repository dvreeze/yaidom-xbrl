<schema
  xmlns="http://www.w3.org/2001/XMLSchema"
  xmlns:xbrli="http://www.xbrl.org/2003/instance"
  xmlns:link="http://www.xbrl.org/2003/linkbase"
  xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:gen="http://xbrl.org/2008/generic"
  xmlns:test="http://abc.com/test"
  targetNamespace="http://abc.com/test"
  elementFormDefault="qualified"
  attributeFormDefault="unqualified">
  <annotation>
    <appinfo>
  	  <link:linkbaseRef xlink:type="simple" 
       xlink:href="test-linkbase.xml" 
       xlink:arcrole="http://www.w3.org/1999/xlink/properties/linkbase"/>
      <link:roleType
        roleURI="http://abc.com/role/test-link-role"
        id="test-link-role">
        <link:usedOn>test:testLink</link:usedOn>
      </link:roleType>
      <link:arcroleType
        arcroleURI="http://abc.com/arcrole/test-arc-role"
        id="test-arc-role"
        cyclesAllowed="any" >
        <link:usedOn>test:testArc</link:usedOn>
      </link:arcroleType>
    </appinfo>
  </annotation>
  <import
    namespace="http://www.xbrl.org/2003/instance"
    schemaLocation="http://www.xbrl.org/2003/xbrl-instance-2003-12-31.xsd" />
  <import 
    namespace="http://www.xbrl.org/2003/linkbase" 
    schemaLocation="http://www.xbrl.org/2003/xbrl-linkbase-2003-12-31.xsd"/>
  <import 
    namespace="http://www.xbrl.org/2003/XLink" 
    schemaLocation="http://www.xbrl.org/2003/xl-2003-12-31.xsd"/>
  <import 
    namespace="http://xbrl.org/2008/generic" 
    schemaLocation="http://www.xbrl.org/2008/generic-link.xsd"/>

  <!-- custom link and arc elements -->
  <element 
    name="testLink" 
    substitutionGroup="gen:link" 
    type="gen:linkTypeWithOpenAttrs"/>
  <element 
    name="testArc" 
    substitutionGroup="gen:arc" 
    type="gen:genericArcType"/>
    
  <!-- arc attribute of an integer -->
  <attribute
    id="testArcInt"
    name="testArcInt"
    type="integer" />
    
  <!-- link attribute of a date -->
  <attribute
    id="testLinkDate"
    name="testLinkDate"
    type="date" />
    
  <!-- fact reporting xbrli item concepts -->
  <element
    id="A"
    name="A"
    type="xbrli:stringItemType"
    substitutionGroup="xbrli:item"
    xbrli:periodType="instant"
    nillable="true" />
  <element
    id="B"
    name="B"
    type="xbrli:stringItemType"
    substitutionGroup="xbrli:item"
    xbrli:periodType="instant"
    nillable="true" />
  <element
    id="C"
    name="C"
    type="xbrli:stringItemType"
    substitutionGroup="xbrli:item"
    xbrli:periodType="instant"
    nillable="true" />
</schema>