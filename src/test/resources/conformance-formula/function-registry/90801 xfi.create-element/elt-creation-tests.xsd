<schema
  xmlns="http://www.w3.org/2001/XMLSchema"
  xmlns:xbrli="http://www.xbrl.org/2003/instance"
  xmlns:link="http://www.xbrl.org/2003/linkbase"
  xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:eg="http://xbrl.org/formula/conformance/example"
  xmlns:xbrldt="http://xbrl.org/2005/xbrldt"
  targetNamespace="http://xbrl.org/formula/conformance/example"
  elementFormDefault="qualified"
  attributeFormDefault="unqualified">
  <annotation>
    <appinfo>
      <link:roleType
        roleURI="http://xbrl.org/formula/conformance/example/role/dimensions"
        id="linkClosedNoDims">
        <link:usedOn>link:definitionLink</link:usedOn>
      </link:roleType>
      <link:roleType
        roleURI="http://xbrl.org/formula/conformance/example/role/linkOpenSegOpenScen"
        id="linkOpenSeg">
        <link:usedOn>link:definitionLink</link:usedOn>
      </link:roleType>
      <link:linkbaseRef xlink:type="simple"
        xlink:role="http://www.xbrl.org/2003/role/definitionLinkbaseRef"
        xlink:arcrole="http://www.w3.org/1999/xlink/properties/linkbase"
        xlink:href="elt-creation-tests-definition.xml"
        xlink:title="Definition Links, all" />
        
      <!-- no longer relevant as the function has been renamed to xfi:create-element
      <link:linkbaseRef xlink:type="simple"
        xlink:arcrole="http://www.w3.org/1999/xlink/properties/linkbase"
        xlink:href="xfxc-element-function-definition.xml"
        xlink:title="xfxc:element Function Definition (not required after accepted into registry)" />
      -->
      
    </appinfo>
  </annotation>
  <import
    namespace="http://www.xbrl.org/2003/instance"
    schemaLocation="http://www.xbrl.org/2003/xbrl-instance-2003-12-31.xsd" />
  <import
    namespace="http://xbrl.org/2005/xbrldt"
    schemaLocation="http://www.xbrl.org/2005/xbrldt-2005.xsd" />
  <import
    namespace="http://xbrl.org/formula/conformance/example-elements"
    schemaLocation="elt-creation-elts.xsd" />
  <element
    id="eg_ExplDim1"
    name="ExplDim1"
    type="xbrli:stringItemType"
    substitutionGroup="xbrldt:dimensionItem"
    xbrli:periodType="instant"
    abstract="true"
    nillable="true" />
  <element
    id="eg_ExplDim1Dom"
    name="ExplDim1Dom"
    type="xbrli:stringItemType"
    substitutionGroup="xbrli:item"
    xbrli:periodType="instant"
    abstract="true"
    nillable="true" />
  <element
    id="eg_ExplDim1Mbr1"
    name="ExplDim1Mbr1"
    type="xbrli:stringItemType"
    substitutionGroup="xbrli:item"
    xbrli:periodType="instant"
    abstract="true"
    nillable="true" />
  <element
    id="typedDim1Value"
    name="typedDim1Value"
    type="string" />
  <element
    id="eg_TypedDim1"
    name="TypedDim1"
    type="xbrli:stringItemType"
    substitutionGroup="xbrldt:dimensionItem"
    xbrli:periodType="instant"
    abstract="true"
    nillable="true"
    xbrldt:typedDomainRef="#typedDim1Value" />
  <element
    id="typedDim2Value"
    name="typedDim2Value">
    <complexType>
      <complexContent>
        <restriction
          base="anyType">
          <attribute
            name="typeDim2Att1"
            type="string" />
          <attribute
            name="typeDim2Att2"
            type="integer" />
        </restriction>
      </complexContent>
    </complexType>
  </element>
  <element
    id="eg_TypedDim2"
    name="TypedDim2"
    type="xbrli:stringItemType"
    substitutionGroup="xbrldt:dimensionItem"
    xbrli:periodType="instant"
    abstract="true"
    nillable="true"
    xbrldt:typedDomainRef="#typedDim2Value" />
  <element
    id="eg_HyperCube"
    name="HyperCube"
    type="xbrli:stringItemType"
    substitutionGroup="xbrldt:hypercubeItem"
    xbrli:periodType="instant"
    abstract="true"
    nillable="true" />
  <element
    id="eg_PriItem"
    name="PriItem"
    type="xbrli:stringItemType"
    substitutionGroup="xbrli:item"
    xbrli:periodType="instant"
    nillable="true" />
  <element id="t" name="t">
    <complexType>
      <complexContent mixed="true">
        <restriction base="anyType">
          <sequence>
            <any minOccurs="0" maxOccurs="unbounded"/>
          </sequence>
          <anyAttribute />
        </restriction>
      </complexContent>
    </complexType>
  </element>
  <attribute id="a" name="a" type="string" />
</schema>