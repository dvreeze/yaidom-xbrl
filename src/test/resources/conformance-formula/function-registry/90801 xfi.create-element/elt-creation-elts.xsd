<schema
  xmlns="http://www.w3.org/2001/XMLSchema"
  xmlns:xbrli="http://www.xbrl.org/2003/instance"
  xmlns:link="http://www.xbrl.org/2003/linkbase"
  xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:eg-elt="http://xbrl.org/formula/conformance/example-elements"
  xmlns:xbrldt="http://xbrl.org/2005/xbrldt"
  targetNamespace="http://xbrl.org/formula/conformance/example-elements"
  elementFormDefault="qualified"
  attributeFormDefault="qualified">
  <import
    namespace="http://www.xbrl.org/2003/instance"
    schemaLocation="http://www.xbrl.org/2003/xbrl-instance-2003-12-31.xsd" />
  <import
    namespace="http://xbrl.org/2005/xbrldt"
    schemaLocation="http://www.xbrl.org/2005/xbrldt-2005.xsd" />
  <element
    id="typedDim3Value"
    name="typedDim3Value">
    <complexType>
      <complexContent>
        <restriction
          base="anyType">
          <attribute
            name="typeDim3Att1"
            type="string" />
          <attribute
            name="typeDim3Att2"
            type="integer" />
        </restriction>
      </complexContent>
    </complexType>
  </element>
  <element
    id="eg_TypedDim3"
    name="TypedDim3"
    type="xbrli:stringItemType"
    substitutionGroup="xbrldt:dimensionItem"
    xbrli:periodType="instant"
    abstract="true"
    nillable="true"
    xbrldt:typedDomainRef="#typedDim3Value" />
  <element
    id="typedDim4Value"
    name="typedDim4Value">
    <complexType>
      <complexContent>
        <restriction
          base="anyType">
          <choice
            minOccurs="1"
            maxOccurs="unbounded">
            <element
              ref="eg-elt:a"
              minOccurs="0"
              maxOccurs="1" />
            <element
              ref="eg-elt:b"
              minOccurs="0"
              maxOccurs="1" />
          </choice>
          <attribute
            name="typeDim4Att1"
            type="string" />
        </restriction>
      </complexContent>
    </complexType>
  </element>
  <element
    id="a"
    name="a"
    type="string"/>
  <element
    id="b"
    name="b"
    type="integer"/>
  <element
    id="eg_TypedDim4"
    name="TypedDim4"
    type="xbrli:stringItemType"
    substitutionGroup="xbrldt:dimensionItem"
    xbrli:periodType="instant"
    abstract="true"
    nillable="true"
    xbrldt:typedDomainRef="#typedDim4Value" />
  <element
    id="ScenarioNondim1Value"
    name="scenarioNondim1Value">
    <complexType>
      <complexContent>
        <restriction
          base="anyType">
          <choice
            minOccurs="1"
            maxOccurs="unbounded">
            <element
              ref="eg-elt:a"
              minOccurs="0"
              maxOccurs="1" />
            <element
              ref="eg-elt:b"
              minOccurs="0"
              maxOccurs="1" />
          </choice>
          <attribute
            name="nondim1Att1"
            type="string" />
        </restriction>
      </complexContent>
    </complexType>
  </element>
</schema>