<testcase
  xmlns="http://xbrl.org/2008/conformance"
  xmlns:xbrli="http://www.xbrl.org/2003/instance"
  xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xbrl.org/2008/conformance  ../../infrastructure/test.xsd">
  <creator>
    <name>Herm Fischer</name>
    <email>fischer@markv.com</email>
  </creator>

  <number>90801</number>

  <name>xfi:create-element</name>

  <description>
    Tests xfi:create-element function when used in formula XPath expressions for output instance typed dimension and OCC element rules.
  </description>

  <variation id="V-01">
    <name>Creation of a typed dimension with string value</name>
    <description>
        A formula creates two output facts with string valued typed dimension.
     </description>
      
    <data>
      <schema>elt-creation-tests.xsd</schema>
      <!-- minimal definition linkbase has open hypercube in the context segment -->
      <linkbase>elt-creation-tests-definition.xsd</linkbase>
      <linkbase>create-typed-dim-string-formula.xml</linkbase>
      <instance readMeFirst="true">create-typed-dim-string-instance.xml</instance>
    </data>
    <result>
      <instance>create-typed-dim-string-result.xml</instance>
    </result>
  </variation>

  <variation id="V-02">
    <name>Creation of a typed dimension with unqualified attributes values</name>
    <description>
        A formula creates two output facts with unqualified attributes in a typed dimension.
     </description>
      
    <data>
      <schema>elt-creation-tests.xsd</schema>
      <linkbase>elt-creation-tests-definition.xsd</linkbase>
      <linkbase>create-typed-dim-unqual-attrs-formula.xml</linkbase>
      <instance readMeFirst="true">create-typed-dim-unqual-attrs-instance.xml</instance>
    </data>
    <result>
      <instance>create-typed-dim-unqual-attrs-result.xml</instance>
    </result>
  </variation>

  <variation id="V-03">
    <name>Creation of a typed dimension with qualified attributes values</name>
    <description>
        A formula creates two output facts with qualified attributes in a typed dimension.
     </description>
      
    <data>
      <schema>elt-creation-tests.xsd</schema>
      <linkbase>elt-creation-tests-definition.xsd</linkbase>
      <linkbase>create-typed-dim-qual-attrs-formula.xml</linkbase>
      <instance readMeFirst="true">create-typed-dim-qual-attrs-instance.xml</instance>
    </data>
    <result>
      <instance>create-typed-dim-qual-attrs-result.xml</instance>
    </result>
  </variation>

  <variation id="V-04">
    <name>Creation of a typed dimension with structure values</name>
    <description>
        A formula creates two output facts with a structure containing subelements and an attribute.
     </description>
      
    <data>
      <schema>elt-creation-tests.xsd</schema>
      <linkbase>elt-creation-tests-definition.xsd</linkbase>
      <linkbase>create-typed-dim-structure-formula.xml</linkbase>
      <instance readMeFirst="true">create-typed-dim-structure-instance.xml</instance>
    </data>
    <result>
      <instance>create-typed-dim-structure-result.xml</instance>
    </result>
  </variation>

  <variation id="V-05">
    <name>Creation of an OCC nondimensional segment with structure values</name>
    <description>
        A formula creates two output facts with an OCC nondimensional structure containing subelements and an attribute.
     </description>
      
    <data>
      <schema>elt-creation-tests.xsd</schema>
      <linkbase>elt-creation-tests-definition.xsd</linkbase>
      <linkbase>create-OCC-seg-structure-formula.xml</linkbase>
      <instance readMeFirst="true">create-OCC-seg-structure-instance.xml</instance>
    </data>
    <result>
      <instance>create-OCC-seg-structure-result.xml</instance>
    </result>
  </variation>


</testcase>


