  <variation id="V-01">
    <name>Works for an instant concept.</name>
    <documentation>
      <xhtml:p>
        Tests that the function works for an instant concept.
      </xhtml:p>
     </documentation>
    <inputs>
      <schema xlink:type="simple" xlink:href="period-type.xsd"/>
      <instance readMeFirst="true" id="V01" xlink:type="simple" xlink:href="instance.xml"/>
      <cfcn:call file="V01">
        xfi:concept-period-type(node-name(/xbrli:xbrl/concept:c1[@contextRef='context-instant'][1]))
      </cfcn:call>
    </inputs>
    <outputs>
      <cfcn:test>
         count( $result ) eq 1
			and
         $result eq 'instant'
      </cfcn:test>
    </outputs>
  </variation>

  <variation id="V-02">
    <name>Works for a duration concept.</name>
    <documentation>
      <xhtml:p>
        Tests that the function works for an duration concept.
      </xhtml:p>
     </documentation>
    <inputs>
      <schema xlink:type="simple" xlink:href="period-type.xsd"/>
      <instance readMeFirst="true" id="V02" xlink:type="simple" xlink:href="instance.xml"/>
      <cfcn:call file="V02">
        xfi:concept-period-type(node-name(/xbrli:xbrl/concept:c2[@contextRef='context-forever'][1]))
      </cfcn:call>
    </inputs>
    <outputs>
      <cfcn:test>
         count( $result ) eq 1
			and
         $result eq 'duration'
      </cfcn:test>
    </outputs>
  </variation>

