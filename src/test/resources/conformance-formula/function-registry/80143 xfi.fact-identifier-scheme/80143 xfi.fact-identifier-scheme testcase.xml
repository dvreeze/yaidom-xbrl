  <number>80143</number>

  <name>Function test xfi:fact-identifier-scheme</name>

  <documentation>
    <xhtml:p>
      Tests of function xfi:identifier-scheme($item as schema-element(xbrli:item)) as xs:anyURI
      </xhtml:p>
  </documentation>

  <reference xlink:type="simple" xlink:href="http://www.xbrl.org/Specification/XF-PWD-2006-12-07.htm#_Toc152670690"/>

  <variation id="V-01">
    <name>Returns correct token for fact item</name>
    <documentation>
      <xhtml:p>
      </xhtml:p>
    </documentation>

    <inputs>
      <schema xlink:type="simple" xlink:href="entity-tests.xsd"/>
      <instance readMeFirst="true" id="V01" xlink:type="simple" xlink:href="entity-instance.xml"/>
      <cfcn:call file="V01">
         xfi:fact-identifier-scheme( /xbrli:xbrl/concept:c1[1] )
      </cfcn:call>
    </inputs>
    <outputs>
      <cfcn:test>
         ($result instance of element(*,xs:anyURI) or $result instance of xs:anyURI) and 
         count( $result ) eq 1 and 
         $result eq xs:anyURI('http://xbrl.org/entity/identification/scheme')
      </cfcn:test>
    </outputs>
  </variation>

  <variation id="V-02">
    <name>Entity identifier value for context-2</name>
    <documentation>
      <xhtml:p>
      </xhtml:p>
    </documentation>
    <inputs>
      <schema xlink:type="simple" xlink:href="entity-tests.xsd"/>
      <instance readMeFirst="true" id="V02" xlink:type="simple" xlink:href="entity-instance.xml"/>
      <cfcn:call file="V02">
         xfi:fact-identifier-scheme( /xbrli:xbrl/concept:c1[2] )
      </cfcn:call>
    </inputs>
    <outputs>
      <cfcn:test>
         ($result instance of element(*,xs:anyURI) or $result instance of xs:anyURI) and 
         count( $result ) eq 1 and 
         $result eq xs:anyURI('http://xbrl.org/entity/identification/scheme2')
      </cfcn:test>
    </outputs>
  </variation>

  <variation id="V-03">
    <name>Wrong node to function</name>
    <documentation>
      <xhtml:p>
        XPath-defined error for node type incorrect.
      </xhtml:p>
    </documentation>
    <inputs>
      <schema xlink:type="simple" xlink:href="entity-tests.xsd"/>
      <instance readMeFirst="true" id="V03" xlink:type="simple" xlink:href="entity-instance.xml"/>
      <cfcn:call file="V03">
         xfi:fact-identifier-scheme( /xbrli:xbrl/xbrli:context[2] )
      </cfcn:call>
    </inputs>
    <outputs>
      <error>xqt-err:XPTY0004</error>
    </outputs>
  </variation>  

  <variation id="V-04">
    <name>A sequence of identifiers as argument</name>
    <documentation>
      <xhtml:p>
        XPath-defined error for node type incorrect arity of argument.
      </xhtml:p>
    </documentation>
    <inputs>
      <schema xlink:type="simple" xlink:href="entity-tests.xsd"/>
      <instance readMeFirst="true" id="V04" xlink:type="simple" xlink:href="entity-instance.xml"/>
      <cfcn:call file="V04">
         xfi:fact-identifier-scheme( /xbrli:xbrl/concept:c1[true()] )
      </cfcn:call>
    </inputs>
    <outputs>
      <error>xqt-err:XPTY0004</error>
    </outputs>
  </variation>

  <revisions>
