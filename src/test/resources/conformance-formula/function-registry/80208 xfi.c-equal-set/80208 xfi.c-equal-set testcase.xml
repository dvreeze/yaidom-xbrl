  http://xbrl.org/2008/function ../schemas/function.xsd
  http://xbrl.org/2008/conformance ../schemas/conformance.xsd
  http://xbrl.org/2008/conformance/function ../schemas/conformanceFunction.xsd
  ">
  <owners>
    <reg:owner id="herm">
      <reg:name>Herm Fischer</reg:name>
      <reg:affiliation>UBMatrix / Mark V Systems</reg:affiliation>
      <reg:email>fischer@markv.com</reg:email>
     <reg:assumedOwnership moment="2008-02-19T00:00:00" />
    </reg:owner>
    <reg:owner id="rna">
      <reg:name>Richard Ashby</reg:name>
      <reg:affiliation>CoreFiling Ltd</reg:affiliation>
      <reg:email>rna@corefiling.com</reg:email>
     <reg:assumedOwnership moment="2012-11-30T00:00:00" />
    </reg:owner>
  </owners>

  <number>80208</number>

  <name>Function test xfi:c-equal-set</name>

  <documentation>
    <xhtml:p>
      Tests of function xfi:c-equal-set($left as node()*, $right as node()*) as xs:boolean
      </xhtml:p>
  </documentation>

  <reference xlink:type="simple" xlink:href="http://www.xbrl.org/Specification/XF-PWD-2006-12-07.htm#_Toc152670710"/>

  <variation id="V-01">
    <name>Returns c-equality of nodes equal and in order</name>
    <documentation>
      <xhtml:p>
      </xhtml:p>
    </documentation>

    <inputs>
      <schema xlink:type="simple" xlink:href="c-equal.xsd"/>
      <linkbase xlink:type="simple" xlink:href="c-equal-definition.xml"/>
      <instance readMeFirst="true" id="V01" xlink:type="simple" xlink:href="c-equal-instance.xml"/>
      <cfcn:call file="V01">
         xfi:c-equal-set( (//c:P1[@id eq 'V01-left'], //c:P3[@id eq 'V07-left']),
                      (//c:P3[@id eq 'V07-left'], //c:P1[@id eq 'V01-right']) )
      </cfcn:call>
    </inputs>
    <outputs>
      <cfcn:test>
         $result instance of xs:boolean and 
         count( $result ) eq 1 and
         $result eq true()
      </cfcn:test>
    </outputs>
  </variation>

  <variation id="V-02">
    <name>Same as V-01 but typed dimension elements in different order</name>
    <documentation>
      <xhtml:p>
      </xhtml:p>
    </documentation>
    <inputs>
      <schema xlink:type="simple" xlink:href="c-equal.xsd"/>
      <linkbase xlink:type="simple" xlink:href="c-equal-definition.xml"/>
      <instance readMeFirst="true" id="V02" xlink:type="simple" xlink:href="c-equal-instance.xml"/>
      <cfcn:call file="V02">
         xfi:c-equal-set( (//c:P1[@id eq 'V01-left'], //c:P3[@id eq 'V07-left']),
                      (//c:P1[@id eq 'V03-right'], //c:P3[@id eq 'V07-left']) )
      </cfcn:call>
    </inputs>
    <outputs>
      <cfcn:test>
         $result instance of xs:boolean and 
         count( $result ) eq 1 and
         $result eq false()
      </cfcn:test>
    </outputs>
  </variation>

  <variation id="V-03">
    <name>Same as V-01 but arguments are sequences</name>
    <documentation>
      <xhtml:p>
      </xhtml:p>
    </documentation>
    <inputs>
      <schema xlink:type="simple" xlink:href="c-equal.xsd"/>
      <linkbase xlink:type="simple" xlink:href="c-equal-definition.xml"/>
      <instance readMeFirst="true" id="V03" xlink:type="simple" xlink:href="c-equal-instance.xml"/>
      <cfcn:call file="V03">
         xfi:c-equal-set( ( //c:P1[@id eq 'V01-left'], //c:P3[@id eq 'V07-left']),
                      ( //c:P1[@id eq 'V01-right'], //c:P3[@id eq 'V07-left']) )
      </cfcn:call>
    </inputs>
    <outputs>
      <cfcn:test>
         $result instance of xs:boolean and 
         count( $result ) eq 1 and
         $result eq true()
      </cfcn:test>
    </outputs>
  </variation>

  <variation id="V-04">
    <name>Same as V-01 but arguments are sequences not same length</name>
    <documentation>
      <xhtml:p>
      </xhtml:p>
    </documentation>
    <inputs>
      <schema xlink:type="simple" xlink:href="c-equal.xsd"/>
      <linkbase xlink:type="simple" xlink:href="c-equal-definition.xml"/>
      <instance readMeFirst="true" id="V04" xlink:type="simple" xlink:href="c-equal-instance.xml"/>
      <cfcn:call file="V04">
         xfi:c-equal-set( ( //c:P1[@id eq 'V01-left'], //c:P3[@id eq 'V07-left']),
                      ( //c:P1[@id eq 'V01-right'] ) )
      </cfcn:call>
    </inputs>
    <outputs>
      <cfcn:test>
         $result instance of xs:boolean and 
         count( $result ) eq 1 and
         $result eq false()
      </cfcn:test>
    </outputs>
  </variation>

  <variation id="V-05">
    <name>Wrong node to function</name>
    <documentation>
      <xhtml:p>
        XPath-defined error for node type incorrect.
      </xhtml:p>
    </documentation>
    <inputs>
      <schema xlink:type="simple" xlink:href="c-equal.xsd"/>
      <linkbase xlink:type="simple" xlink:href="c-equal-definition.xml"/>
      <instance readMeFirst="true" id="V05" xlink:type="simple" xlink:href="c-equal-instance.xml"/>
      <cfcn:call file="V05">
         xfi:c-equal-set( (//c:P1[@id eq 'V01-left']), ('foo') )
      </cfcn:call>
    </inputs>
    <outputs>
      <error>xqt-err:XPTY0004</error>
    </outputs>
  </variation>  

  <variation id="V-06">
    <name>Non item node to function</name>
    <documentation>
      <xhtml:p>
        XBRL-defined error for node type incorrect.
      </xhtml:p>
    </documentation>
    <inputs>
      <schema xlink:type="simple" xlink:href="c-equal.xsd"/>
      <linkbase xlink:type="simple" xlink:href="c-equal-definition.xml"/>
      <instance readMeFirst="true" id="V06" xlink:type="simple" xlink:href="c-equal-instance.xml"/>
      <cfcn:call file="V06">
         xfi:c-equal-set( (//xbrli:context[@id='c-V01-left']), (//c:P1[@id eq 'V01-left']) )
      </cfcn:call>
    </inputs>
    <outputs>
      <error>xfie:NodeIsNotXbrlItem</error>
    </outputs>
  </variation>  

  <variation id="V-07">
    <name>A sequence of concepts as argument</name>
    <documentation>
      <xhtml:p>
        XPath-defined error for node type incorrect number of arguments.
      </xhtml:p>
    </documentation>
    <inputs>
      <schema xlink:type="simple" xlink:href="c-equal.xsd"/>
      <linkbase xlink:type="simple" xlink:href="c-equal-definition.xml"/>
      <instance readMeFirst="true" id="V07" xlink:type="simple" xlink:href="c-equal-instance.xml"/>
      <cfcn:call file="V07">
         xfi:c-equal-set( //xbrli:xbrl/c:P1[starts-with(@id,'V0')] )
      </cfcn:call>
    </inputs>
    <outputs>
      <error>xqt-err:XPST0017</error>
    </outputs>
  </variation>
  
  <variation id="V-08">
    <name>Performs set de-duplication of sequences</name>
    <documentation>
      <xhtml:p>
      Works for sequences containing duplicate entries, which are equal when compared as sets
      </xhtml:p>
    </documentation>
    <inputs>
      <schema xlink:type="simple" xlink:href="c-equal.xsd"/>
      <instance readMeFirst="true" id="V08" xlink:type="simple" xlink:href="c-equal-instance.xml"/>
      <!-- Second sequence contains duplicated node, albeit obtained differently -->
      <cfcn:call file="V08">
         xfi:c-equal-set(
            ( //c:P1[@id='V01-left']),
            ( //c:P1[@id='V01-left'], //c:P1[@contextRef='c-V01-left'])
         )
      </cfcn:call>
    </inputs>
    <outputs>
      <cfcn:test>
         $result eq true()
      </cfcn:test>
    </outputs>
  </variation>

