  <number>80212</number>

  <name>Function test xfi:duplicate-tuple</name>

  <documentation>
    <xhtml:p>
      Tests of function xfi:duplicate-tuple($left as schema-element(xbrli:tuple), $right as schema-element(xbrli:tuple)) as xs:boolean
      </xhtml:p>
    <xhtml:p>
      UsesExample 25 of XBRL Spec v2.1.
      </xhtml:p>
  </documentation>

  <reference xlink:type="simple" xlink:href="http://www.xbrl.org/Specification/XF-PWD-2006-12-07.htm#_Toc152670713"/>

  <variation id="V-01">
    <name>Item, not an tuple</name>
    <documentation>
      <xhtml:p>
        XPath-defined error for node type incorrect.
       </xhtml:p>
    </documentation>

    <inputs>
      <schema xlink:type="simple" xlink:href="example25.xsd"/>
      <instance readMeFirst="true" id="V01" xlink:type="simple" xlink:href="example25-instance.xml"/>
      <cfcn:call file="V01">
         xfi:duplicate-tuple( //s:name[@id eq 'fName'],
                      //s:name[@id eq 'gName'] )
      </cfcn:call>
    </inputs>
    <outputs>
      <error>xqt-err:XPTY0004</error>
    </outputs>
  </variation>

  <variation id="V-02">
    <name>Item, not an tuple</name>
    <documentation>
      <xhtml:p>
        XPath-defined error for node type incorrect.
       </xhtml:p>
    </documentation>

    <inputs>
      <schema xlink:type="simple" xlink:href="example25.xsd"/>
      <instance readMeFirst="true" id="V02" xlink:type="simple" xlink:href="example25-instance.xml"/>
      <cfcn:call file="V02">
         xfi:duplicate-tuple( //s:name[@id eq 'bName'],
                      //s:name[@id eq 'cName'] )
      </cfcn:call>
    </inputs>
    <outputs>
      <error>xqt-err:XPTY0004</error>
    </outputs>
  </variation>

  <variation id="V-03">
    <name>Item, not an tuple</name>
    <documentation>
      <xhtml:p>
        XPath-defined error for node type incorrect.
