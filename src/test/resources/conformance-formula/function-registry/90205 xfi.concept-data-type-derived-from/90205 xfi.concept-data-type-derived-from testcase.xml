  <number>90205</number>

  <name>Function test for xfi:data-type-derived-from</name>

  <documentation>
    <xhtml:p>
      Tests various pairs of data types that are related and unrelated
      to eachother by derivation.
      </xhtml:p>
  </documentation>

  <reference xlink:type="simple" xlink:href="http://www.xbrl.org/Specification/XF-PWD-2006-12-07.htm#_Toc152670666"/>

  <variation id="V-01">
    <name>Return true for xbrli:stringItemType and xs:string data types</name>
    <documentation>
      <xhtml:p>
      </xhtml:p>
    </documentation>
    <inputs>
      <schema readMeFirst="true" id="V01" xlink:type="simple" xlink:href="concept-data-type.xsd"/>
      <cfcn:call file="V01">
        xfi:concept-data-type-derived-from(
          QName('http://xbrl.org/formula/conformance/example','cStr'),
          QName('http://www.w3.org/2001/XMLSchema','string')
        )
      </cfcn:call>
    </inputs>
    <outputs>
      <cfcn:test>
         $result eq fn:true() 
      </cfcn:test>
    </outputs>
  </variation>

  <variation id="V-02">
    <name>Return true for xbrli:stringItemType and xbrli:stringItemType data types</name>
    <documentation>
      <xhtml:p>
      </xhtml:p>
    </documentation>
    <inputs>
      <schema readMeFirst="true" id="V02" xlink:type="simple" xlink:href="concept-data-type.xsd"/>
      <cfcn:call file="V02">
        xfi:concept-data-type-derived-from(
          QName('http://xbrl.org/formula/conformance/example','cStr'),
          QName('http://www.xbrl.org/2003/instance','stringItemType')
        )
      </cfcn:call>
    </inputs>
    <outputs>
      <cfcn:test>
         $result eq fn:true() 
      </cfcn:test>
    </outputs>
  </variation>

  <variation id="V-03">
    <name>Return false for xbrli:stringItemType and xs:decimal data types</name>
    <documentation>
      <xhtml:p>
      </xhtml:p>
    </documentation>
    <inputs>
      <schema readMeFirst="true" id="V03" xlink:type="simple" xlink:href="concept-data-type.xsd"/>
      <cfcn:call file="V03">
        xfi:concept-data-type-derived-from(
          QName('http://xbrl.org/formula/conformance/example','cStr'),
          QName('http://www.w3.org/2001/XMLSchema','decimal')
        )
      </cfcn:call>
    </inputs>
    <outputs>
      <cfcn:test>
         $result eq fn:false() 
      </cfcn:test>
    </outputs>
  </variation>

  <variation id="V-04">
    <name>Return false for xbrli:stringItemType and xbrli:monetaryItemType data types</name>
    <documentation>
      <xhtml:p>
      </xhtml:p>
    </documentation>
    <inputs>
      <schema readMeFirst="true" id="V04" xlink:type="simple" xlink:href="concept-data-type.xsd"/>
      <cfcn:call file="V04">
        xfi:concept-data-type-derived-from(
          QName('http://xbrl.org/formula/conformance/example','cStr'),
          QName('http://www.xbrl.org/2003/instance','monetaryItemType')
        )
      </cfcn:call>
    </inputs>
    <outputs>
      <cfcn:test>
         $result eq fn:false() 
      </cfcn:test>
    </outputs>
  </variation>

  <variation id="V-05">
    <name>Return false for xbrli:floatItemType and xbrli:decimalItemType data types</name>
    <documentation>
      <xhtml:p>
      </xhtml:p>
    </documentation>
    <inputs>
      <schema readMeFirst="true" id="V05" xlink:type="simple" xlink:href="concept-data-type.xsd"/>
      <cfcn:call file="V05">
        xfi:concept-data-type-derived-from(
          QName('http://xbrl.org/formula/conformance/example','cFlt'),
          QName('http://www.xbrl.org/2003/instance','decimalItemType')
        )
      </cfcn:call>
    </inputs>
    <outputs>
      <cfcn:test>
         $result eq fn:false() 
      </cfcn:test>
    </outputs>
  </variation>

