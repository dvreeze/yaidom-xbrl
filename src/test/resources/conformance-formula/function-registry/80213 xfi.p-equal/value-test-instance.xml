      <identifier scheme="http://xbrl.org/entity/identification/scheme">01</identifier>
    </entity>
    <period>
      <instant>2007-12-31</instant>
    </period>
  </context>
  
  <context id="context-2">
    <entity>
      <identifier scheme="http://xbrl.org/entity/identification/scheme">02</identifier>
    </entity>
    <period>
      <instant>2007-12-31</instant>
    </period>
  </context>
  
  <unit id="unit-squareMiles">
    <measure>c:squareMiles</measure>
  </unit>
  
  <unit id="unit-acres">
    <measure>c:acres</measure>
  </unit>
  
  <unit id="unit-hectares">
    <measure>c:hectares</measure>
  </unit>
  
  <unit id="unit-speedMPH">
    <divide>
       <unitNumerator>
         <measure>c:miles</measure>
       </unitNumerator>
       <unitDenominator>
         <measure>c:hours</measure>
       </unitDenominator>
    </divide>
  </unit>

  <unit id="unit-speedKMH">
    <divide>
       <unitNumerator>
         <measure>c:kilometers</measure>
       </unitNumerator>
       <unitDenominator>
         <measure>c:hours</measure>
       </unitDenominator>
    </divide>
  </unit>

  <unit id="unit-acceleration">
    <divide>
       <unitNumerator>
         <measure>c:meters</measure>
       </unitNumerator>
       <unitDenominator>
         <measure>c:seconds</measure>
         <measure>c:seconds</measure>
       </unitDenominator>
    </divide>
  </unit>

  <unit id="unit-wacky-force1">
    <divide>
       <unitNumerator>
         <measure>c:kilograms</measure>
         <measure>c:meters</measure>
       </unitNumerator>
       <unitDenominator>
         <measure>c:seconds</measure>
         <measure>c:hours</measure>
       </unitDenominator>
    </divide>
  </unit>

  <unit id="unit-wacky-force2">
    <divide>
       <unitNumerator>
         <measure>c:meters</measure>
         <measure>c:kilograms</measure>
       </unitNumerator>
       <unitDenominator>
         <measure>c:hours</measure>
         <measure>c:seconds</measure>
       </unitDenominator>
    </divide>
  </unit>

  <unit id="unit-less-wacky-force">
    <divide>
       <unitNumerator>
         <measure>c:meters</measure>
         <measure>c:kilograms</measure>
       </unitNumerator>
       <unitDenominator>
         <measure>c:seconds</measure>
         <measure>c:seconds</measure>
       </unitDenominator>
    </divide>
  </unit>

  <c:a id="t1" decimals="3" contextRef="context-1" unitRef="unit-wacky-force1">1001.126</c:a>
